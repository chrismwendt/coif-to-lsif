{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es5"],
    "outDir": "out",
    "strict": true,
    "target": "es5",
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
